{
  "version": "1.0.0",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "strapi-v5",
    "bookmarks",
    "query-builder",
    "content-manager",
    "filters",
    "productivity",
    "admin-panel"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist/",
    "strapi-server.js",
    "README.md",
    "LICENSE",
    "COPYRIGHT_NOTICE.txt"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "@types/qs": "^6.14.0",
    "qs": "^6.14.0",
    "react-intl": "^7.1.14"
  },
  "devDependencies": {
    "semantic-release": "^24.2.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.29.0",
    "@strapi/typescript-utils": "^5.29.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "magic-mark",
    "displayName": "MagicMark - Query Builder",
    "description": "Save and apply complex Content Manager queries with one click. Advanced bookmark management with role-based sharing."
  },
  "name": "strapi-plugin-magic-mark",
  "description": "Advanced Query Builder and Bookmark Manager for Strapi v5 - Save complex Content Manager filters with one click",
  "license": "MIT",
  "private": false,
  "author": "Schero A. <124470865+begservice@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/begservice/strapi-plugin-magic-mark.git"
  },
  "bugs": {
    "url": "https://github.com/begservice/strapi-plugin-magic-mark/issues"
  },
  "homepage": "https://github.com/begservice/strapi-plugin-magic-mark#readme"
}
